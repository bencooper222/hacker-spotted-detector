{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ],
  "env": {
    "timezone": "America/Chicago",
    "admin_deny": "negative_squared_cross_mark"
  }
}
